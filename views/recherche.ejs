                                <img src="<%= locals.users[i].profil_img != 0 ? `assets/img/${locals.users[i].profil_img}` : "http://ssl.gstatic.com/accounts/ui/avatar_2x.png" %>"
                                     class="img-responsive" alt="" style="width: 100%;">
                                <ul class="category_desc1">
                                    <% if(locals.users[i].score != 0){ %>
                                        <li class="cate_head">Score:
                                            <% for(let j = 0; j < locals.users[i].score; j++){ %>
                                                <label style="color: #fff;">&#9733;</label>
                                            <% } %>
                                        </li>
                                    <% }else{ %>
                                        <li class="cate_head" style="height: 30px">No score</li>
                                    <% } %>
                                    <div class="clearfix"></div>
                                </ul>
                            </a></div>
                        <br>
                        <br>
                    </div>
                    <% }; %>
                    <div class="clearfix"></div>
                </div>
            </fieldset>
            <fieldset>
                <legend>Les utilisateur avec des interets commun:</legend>

<html>
<head>

    <title>Matcha | Login</title>

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet"/>
    <link href='http://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
    <link href="/assets/css/bootstrap.css" rel='stylesheet' type='text/css'/>
    <link href="/assets/css/style.css" rel='stylesheet' type='text/css'/>
    <link href="/assets/css/notif.css" rel='stylesheet' type='text/css'/>
    <link href="/assets/css/sliders.css" rel='stylesheet' type='text/css'/>

</head>
<body>
<div class="header wow fadeInUp" data-wow-delay="0.4s">
    <div class='container'>
        <div class="header_left">
            <img src="/assets/images/rate.png" alt=""/>
        </div>
        <div class="logo">
            <a href="/"><img src="/assets/images/logo.png" alt=""/>
                <span>Matcha</span>
            </a>
        </div>
        <% if (locals.connected == false){ %>
            <div class="header_right">
                <ul class="header_user_info">
                    <a class="login" href="login">
                        <li class="user_desc"><span class="m_1">Already user ? </span>Login</li>
                        <i class="user"> </i>
                        <div class="clearfix"></div>
                    </a>
                    <div class="clearfix"></div>
                </ul>
                <a href="register" class="btn1 btn-1 btn1-1b">Sign Up</a>
            </div>
        <% }else{ %>
            <div class="header_right">
                <ul class="header_user_info">
                    <a class="login" href="profil">
                        <li class="user_desc">Manage your account</li>
                        <i class="user"> </i>
                        <div class="clearfix"></div>
                    </a>
                    <div class="clearfix"></div>
                </ul>
                <a href="logout" class="btn1 btn-1 btn1-1b">Logout</a>
            </div>
        <% } %>
        <div class="clearfix">
            <!--  TEST NOTIF ---------------------------->

            <div class="container_notif">
                <!-- <a href="#" class="button notificationicon on"><div class="notification"></div></a> -->
                <div class="notification"></div>
                    <!-- <button>Increment Notifications</button> -->
            </div>
            <div class="notif_box">
                <ul id="output_notif">

                </ul>
            </div>
            <!--  TEST NOTIF ---------------------------->
        </div>
    </div>
</div>
<div class="profile_banner">
    <div class="container">
        <h3>Search</h3>
    </div>
</div>
<form action="recherche" method="post">

    <div class="wrapper" style="padding:20px;">
        <input type="checkbox" name="ageBox" value="1" <%= locals.ageChecked == "checked" ? checked = "checked" : "" %>>
        <h4>Age:</h4>
        <div class="extra-controls form-inline">
            <input type="number" name="age1" class="js-input-from form-control"
                   value="<%= locals.age1 != null ? locals.age1 : 18 %>" min="18" max="100"/>
            <input type="number" name="age2" class="js-input-to form-control"
                   value="<%= locals.age2 != null ? locals.age2 : 100 %>" min="18" max="100"/>
        </div>
        <br>
        <input type="checkbox" name="distanceBox"
               value="1" <%= locals.distanceChecked == "checked" ? checked = "checked" : "" %>><h4>Distance:</h4>
        <div class="extra-controls form-inline">
            <input type="number" name="distance1" class="js-input-from form-control"
                   value="<%= locals.distance1 != null ? locals.distance1 : 0 %>" min="0" max="100000"/>
            <input type="number" name="distance2" class="js-input-to form-control"
                   value="<%= locals.distance2 != null ? locals.distance2 : 50 %>" min="0" max="100000"/>
        </div>
        <br>
        <input type="checkbox" name="scoreBox"
               value="1" <%= locals.scoreChecked == "checked" ? checked = "checked" : "" %>><h4>Score:</h4>
        <div class="extra-controls form-inline">
            <input type="number" name="score1" class="js-input-from form-control"
                   value="<%= locals.score1 != null ? locals.score1 : 0 %>" min="0" max="5"/>
            <input type="number" name="score2" class="js-input-to form-control"
                   value="<%= locals.score2 != null ? locals.score2 : 5 %>" min="0" max="5"/>
        </div>
        <br>
        <h4>#Tag:</h4>
        <div class="extra-controls form-inline">
            <input type="text" name="tag" class="js-input-from form-control"
                   value="<%= locals.tag != null ? locals.tag : "" %>"/>
        </div>
    </div>
    </div>
    <button class="btn1 btn-1 btn1-1b" style="float: right" type="submit">Search</button>

</form>

<div class="container">
    <% if(locals.users != 1){ %>
        <% if(locals.sorted == true){ %>

                        <div class="grid_4"><a  href="single?<%= locals.users[i][0].username %>" target="_blank">
                                <h4><span class="m_3"><%= locals.users[i][0].username %></span></h4>
                                <img src="<%= locals.users[i][0].profil_img != 0 ? `assets/img/${locals.users[i][0].profil_img}` : "http://ssl.gstatic.com/accounts/ui/avatar_2x.png" %>"
                                     class="img-responsive" alt="" style="width: 100%;">
                                <ul class="category_desc1">
                                    <% if(locals.users[i][0].score != 0){ %>
                                        <li class="cate_head">Score:
                                            <% for(let j = 0; j < locals.users[i][0].score; j++){ %>
                                                <label style="color: #fff;">&#9733;</label>
                                            <% } %>
                                        </li>
                                    <% }else{ %>
                                        <li class="cate_head" style="height: 30px">No score</li>
                                    <% } %>
